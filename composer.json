{
    "name": "key2info/mailchimp-marketing",
    "description": "Marketing integration for Mailchimp",
    "type": "wordpress-plugin",
    "license": "proprietary",
    "authors": [
        {
            "name": "De Belser Arne",
            "email": "arne@bruno-agency.be"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/Key2info/mailchimp-marketing-client"
        }
